<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.shixuanyoumei.mapper.AdvisoryMapper" >
    <select id="SearchAdvisory" resultType="com.example.shixuanyoumei.entity.Advisory">
        SELECT
          `id`,
          `picPath`,
          `context`,
          `smallContext`
        FROM
          `advisory`
